    "babel-loader": "^8.0.0-beta",
    "babel-plugin-react-css-modules": "^3.4.0",
    "css-loader": "^0.28.11",
    "favicons-webpack-plugin": "^0.0.9",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^3.2.0",
